<%= form_for @store do |s| %>
 <div id="super-container-store">
    <div id="controls">
      <form class="form-horizontal" role="form">
        <div id="text-field-container">
          <div class="form-group">
            <div class="col-lg-10">
              <p>
                <%= s.text_field :name, placeholder: "name" %>
              </p>
            </div>
          </div>
        </div>
        <div id="text-field-container">
          <div class="form-group">
            <div class="col-lg-10">
              <p>
                <%= s.text_field :specialty, placeholder: "specialty" %>
              </p>
            </div>
          </div>
        </div>
        <div id="text-field-container">
          <div class="form-group">
            <div class="col-lg-10">
              <p>
                <%= s.text_field :street, placeholder: "street" %>
              </p>
            </div>
          </div>
        </div>
        <div id="text-field-container">
          <div class="form-group">
            <div class="col-lg-10">
              <p>
                <%= s.text_field :city, placeholder: "city" %>
              </p>
            </div>
          </div>
        </div>
        <div id="text-field-container">
          <div class="form-group">
            <div class="col-lg-10">
              <p>
                <%= s.text_field :state, placeholder: "state" %>
              </p>
            </div>
          </div>
        </div>
        <div id="text-field-container">
          <div class="form-group">
            <div class="col-lg-10">
              <p>
                <%= s.text_field :zip, placeholder: "zip" %>
              </p>
            </div>
          </div>
        </div>
         <div id="text-field-container">
          <div class="form-group">
            <div class="col-lg-10">
              <p>
                <%= s.text_field :phone_number, placeholder: "phone" %>
              </p>
            </div>
          </div>
        </div>
        <div id="text-field-container">
          <div class="form-group">
            <div class="col-lg-10">
              <p>
                <%= s.text_field :email, placeholder: "email" %>
              </p>
            </div>
          </div>
        </div>
        <div id="text-field-container">
          <div class="form-group">
            <div class="col-lg-10">
              <p>
                <%= s.text_field :website, placeholder: "website" %>
              </p>
            </div>
          </div>
        </div>
        <div class="form-group">
          <div class="col-lg-offset-2 col-lg-10">
            <p>
              <%= s.submit %>
            </p>
          </div>
        </div>
    </form>
    <div>
  <div>
<% end %>
